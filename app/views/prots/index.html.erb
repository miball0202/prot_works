
<br>
  <h1>Prot Pickup!</h1>

  <hr>
  <div class="row">
    <br>
    <% @prots.each do |prot| %>
      <div class="col-lg-3 col-md-6 col-sm-12">
        <%= link_to prot.user.nick_name, prot.user, class: "text-dark" %>
        <div class="card border-dark mb-3" style="max-width: 18rem;">
          <div class="card-body">
            <%= link_to prot.title, prot , class: "text-dark" %>
          </div>
        </div>
      </div>
    <% end %>
    <br>
  </div>

<br><br><br><br><br><br>
<div class="halfCircle"></div>
